# profiles.yml.example
# Example dbt profile for Snowflake + Databricks.

school_climate_profile:
  target: snowflake_dev
  outputs:
    snowflake_dev:
      type: snowflake
      account: "{{ env_var('SNOWFLAKE_ACCOUNT') }}"
      user: "{{ env_var('SNOWFLAKE_USER') }}"
      password: "{{ env_var('SNOWFLAKE_PASSWORD') }}"
      role: "{{ env_var('SNOWFLAKE_ROLE') }}"
      warehouse: "{{ env_var('SNOWFLAKE_WAREHOUSE') }}"
      database: "{{ env_var('SNOWFLAKE_DATABASE') }}"
      schema: "{{ env_var('SNOWFLAKE_SCHEMA_DBT', 'BRONZE') }}"
      threads: 4
      client_session_keep_alive: False
      query_tag: "dbt-school-climate-dev"

    databricks_dev:
      type: databricks
      host: "{{ env_var('DATABRICKS_HOST') }}"
      http_path: "{{ env_var('DATABRICKS_HTTP_PATH', '') }}"
      token: "{{ env_var('DATABRICKS_TOKEN') }}"
      catalog: "main"
      schema: "{{ env_var('SNOWFLAKE_SCHEMA_DBT', 'dbt_dylan') }}"
      threads: 4
